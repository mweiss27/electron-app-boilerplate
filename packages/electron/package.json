{
  "name": "electron-app-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "build/development/main.js",
  "scripts": {
    "build:development": "webpack --config ./webpack/electron.webpack.development.js",
    "build:production": "webpack --config ./webpack/electron.webpack.production.js && node scripts/copyfiles.js && npm i --prefix ../../build/electron",
    "clean": "rimraf ./build",
    "electron": "nodemon electron .",
    "wait:client": "wait-on tcp:3000",
    "start": "npm-run-all clean build:development wait:client electron",
    "tsc": "tsc --project ./tsconfig.json",
    "rebuild": "./node_modules/.bin/electron-rebuild"
  },
  "keywords": [],
  "author": "Matt Weiss",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.14",
    "@types/nodemon": "^1.19.1",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5",
    "wait-on": "^6.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "cors": "2.8.5",
    "electron": "^17.0.0",
    "electron-is-dev": "2.0.0",
    "electron-rebuild": "^3.2.7",
    "express": "^4.17.2"
  }
}
